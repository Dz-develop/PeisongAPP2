<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 自适应宽高 -->
    <style name="wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <!-- 宽全屏，高自适应 -->
    <style name="fill_width">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <!-- 宽、高全屏 -->
    <style name="fill">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <!-- 搜房样式 -->
    <style name="Theme.Soufun" parent="@android:style/Theme.Light">
        <item name="android:windowBackground">@drawable/window_bg</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
    </style>
    <!--  -->
    <style name="Theme.Translucent" parent="android:style/Theme.Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <!-- 进度对话框样式 -->
    <style name="Theme.Light.ProcessDialog.Blue" parent="@android:style/Theme.Light.NoTitleBar">

        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
    </style>
    <!-- 对话框样式 -->
    <style name="Theme.Light.Dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:scrollHorizontally">true</item>
    </style>
    <!-- listview样式 -->
    <style name="list">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:dividerHeight">1.0px</item>
        <item name="android:divider">@drawable/divider_line</item>
        <item name="android:fastScrollEnabled">true</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">@color/white</item>
        <item name="android:layout_weight">0.0</item>
    </style>
    <!-- listview样式 -->
    <style name="morelist">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:dividerHeight">0.0px</item>
